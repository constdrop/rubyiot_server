# A sample Gemfile
source "https://rubygems.org"

gem "activerecord"
gem "haml"
gem "rake"
gem "sqlite3"
gem "mysql2"
gem "sinatra"
gem "sinatra-contrib"
gem "sinatra-activerecord", :require => 'sinatra/activerecord'
gem "unicorn"

group :test do
  gem "rspec"
  gem "rack-test"
  gem "json_spec"
  gem "simplecov", require: false
end
